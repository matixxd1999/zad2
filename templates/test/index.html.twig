{% extends 'base.html.twig' %}

{% block title %}Raport{% endblock %}


{% block body %}



    <table class="content-table">
        <thead>
        <tr>
            <th>Lp.</th>
            <th>RCP</th>
            <th>Iniciały</th>
            <th>Ilość</th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        {% for i in repoMsw |batch(2, "no item") %}
            <tr class="active-row">
                <td>{{ index.loop }}</td>
                <td>{{ i.rcp }}</td>
                <td>{{ i.inicial }}</td>
                <td>{{ i.ilosc }}</td>
                <td></td>
            </tr>
        {% endfor %}
        </tbody>
        </head>
    </table>

{% endblock %}

{% block stylesheets %}
    {{ include('i..html.twig')|inline_css }}
{% endblock %}